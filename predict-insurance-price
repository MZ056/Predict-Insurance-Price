    "# Заполняем пропуски"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {
    "execution": {
     "iopub.execute_input": "2021-12-19T13:01:27.708713Z",
     "iopub.status.busy": "2021-12-19T13:01:27.708511Z",
     "iopub.status.idle": "2021-12-19T13:01:27.713251Z",
     "shell.execute_reply": "2021-12-19T13:01:27.712462Z",
     "shell.execute_reply.started": "2021-12-19T13:01:27.708689Z"
    }
   },
   "outputs": [],
   "source": [
    "# Заполняем пропуски в данных\n",
    "\n",
    "def func_fill(data):\n",
    "    if isinstance(data, pd.Series):\n",
    "        try:\n",
    "            if data.dtype is pd.np.dtype(float):\n",
    "                data.fillna(data.median(), inplace=True)\n",
    "            elif data.dtype is pd.np.dtype(int):\n",
    "                data.fillna(data.median(), inplace=True)\n",
    "            elif data.dtype is pd.np.dtype(object):\n",
    "                data.fillna(data.mode()[0], inplace=True)\n",
    "            else:\n",
    "                ('Не нашел указанные типы')\n",
    "        except Exception as ex:\n",
    "            print(f'Поздравляю, у тебя ошибка - {ex}')\n",
    "    else:\n",
    "        print('Series, не DF -_-')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "for col in train.columns:\n",
    "    func_fill(train[col])\n",
    "    \n",
    "for col in test.columns:\n",
    "    func_fill(test[col])"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
