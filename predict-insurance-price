    "# Modeling"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
   "metadata": {
    "execution": {
     "iopub.execute_input": "2021-12-19T13:01:28.232793Z",
     "iopub.status.busy": "2021-12-19T13:01:28.232519Z",
     "iopub.status.idle": "2021-12-19T13:01:28.251666Z",
     "shell.execute_reply": "2021-12-19T13:01:28.250800Z",
     "shell.execute_reply.started": "2021-12-19T13:01:28.232764Z"
    }
   },
   "outputs": [],
   "source": [
    "X = train.iloc[:, :-1]\n",
    "y = train.iloc[:, -1].values\n",
    "\n",
    "X_train, X_test, y_train, y_test = train_test_split(\n",
    "    X, y, test_size=0.2, random_state=RAND)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 32,
   "metadata": {},
   "outputs": [],
   "source": [
    "# DataFrame для записи результатов\n",
    "\n",
    "metrics = pd.DataFrame(\n",
    "    index=('MAE', 'MAPE', 'MSE', 'RMSE', 'R2_adjusted', 'RMSLE'))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "metadata": {},
   "outputs": [],
   "source": [
    "def r2_adjusted(y_true: list, y_pred: list, X_test: np.array) -> float:\n",
    "    \n",
    "    N_objects = len(y_true)\n",
    "    N_features = X_test.shape[1]\n",
    "    r2 = r2_score(y_true, y_pred)\n",
    "    return 1 - (1 - r2) * (N_objects - 1) / (N_objects - N_features - 1)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Baseline"
   ]
